<configuration>
  <connectionStrings>
    <!-- Yuan Data Source .\SQLEXPRESS -->
    <!-- Luis Data Source (localdb)\CGEL -->
    <add name="aspnetdbConnectionString" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=aspnetdb;Integrated Security=True" providerName="System.Data.SqlClient"/>
    <add name="ProyectoMandiolaConnectionString" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=ProyectoMandiola;Integrated Security=True; Column Encryption Setting = Enabled" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None"/>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
  -->
  <system.web>
    <siteMap defaultProvider="web">
      <providers>
        <add name="backend" type="System.Web.XmlSiteMapProvider" siteMapFile="Content/Backend/backend.sitemap"/>
        <add name="web" type="System.Web.XmlSiteMapProvider" siteMapFile="Web.sitemap"/>
      </providers>
    </siteMap>
    <!--<membership defaultProvider="auth">
      <providers>
        <add connectionStringName="aspnetdbConnectionString" applicationName="H-Mandiola-Backend" minRequiredPasswordLength="8" minRequiredNonalphanumericCharacters="0" name="auth" type="System.Web.Security.SqlMembershipProvider" requiresQuestionAndAnswer="false" />
      </providers>
    </membership>-->
    <!--Proveedor para el manejo de roles-->
    <!--<roleManager enabled="true" defaultProvider="rol">
      <providers>
        <add connectionStringName="aspnetdbConnectionString" applicationName="H-Mandiola-Backend" name="rol" type="System.Web.Security.SqlRoleProvider" />
      </providers>
    </roleManager>
    <authentication mode="Forms">
      <forms name="H-Mandiola-Backend" loginUrl="login.aspx" defaultUrl="Default.aspx" timeout="60" protection="All" path="/" />
    </authentication>
    <authorization>
      <deny users="?" />
      <allow users="*" />
